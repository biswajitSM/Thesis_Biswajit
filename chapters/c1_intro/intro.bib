  timestamp	= {2017.12.20},
  url		= {http://dx.doi.org/10.1146/annurev.biochem.67.1.509}
}

@Article{	  weiss1999fluorescence,
  title		= {Fluorescence spectroscopy of single biomolecules.},
  author	= {Weiss, Shimon},
  journal	= {Science (New York, N.Y.)},
  year		= {1999},
  month		= {3},
  number	= {5408},
  pages		= {1676--83},
  volume	= {283},
  abstract	= {Recent advances in single-molecule detection and
		  single-molecule spectroscopy at room temperature by
		  laser-induced fluorescence offer new tools for the study of
		  individual macromolecules under physiological conditions.
		  These tools relay conformational states, conformational
		  dynamics, and activity of single biological molecules to
		  physical observables, unmasked by ensemble averaging.
		  Distributions and time trajectories of these observables
		  can therefore be measured during a reaction without the
		  impossible need to synchronize all the molecules in the
		  ensemble. The progress in applying these tools to
		  biological studies with the use of fluorophores that are
		  site-specifically attached to macromolecules is reviewed.},
  doi		= {10.1126/science.283.5408.1676},
  issn		= {0036-8075},
  owner		= {biswajit},
  timestamp	= {2017.09.05},
  url		= {http:https://doi.org/10.1126/science.283.5408.1676}
}

@Article{	  white1987an,
  title		= {An evaluation of confocal versus conventional imaging of
		  biological structures by fluorescence light microscopy.},
  author	= {White, J G and Amos, W B and Fordham, M},
  journal	= {The Journal of Cell Biology},
  year		= {1987},
  number	= {1},
  pages		= {41--48},
  volume	= {105},
  abstract	= {Scanning confocal microscopes offer improved rejection of
		  out-of-focus noise and greater resolution than conventional
		  imaging. In such a microscope, the imaging and condenser
		  lenses are identical and confocal. These two lenses are
		  replaced by a single lens when epi-illumination is used,
		  making confocal imaging particularly applicable to incident
		  light microscopy. We describe the results we have obtained
		  with a confocal system in which scanning is performed by
		  moving the light beam, rather than the stage. This system
		  is considerably faster than the scanned stage microscope
		  and is easy to use. We have found that confocal imaging
		  gives greatly enhanced images of biological structures
		  viewed with epifluorescence. The improvements are such that
		  it is possible to optically section thick specimens with
		  little degradation in the image quality of interior
		  sections.},
  doi		= {10.1083/jcb.105.1.41},
  issn		= {0021-9525},
  owner		= {biswajit},
  pmid		= {3112165},
  publisher	= {The Journal of Cell Biology},
  timestamp	= {2017.12.20},
  url		= {http://dx.doi.org/10.1083/jcb.105.1.41}
}

