@Article{	  punj2013a,
  title		= {A plasmonic /`antenna-in-box/' platform for enhanced
		  single-molecule analysis at micromolar concentrations},
  author	= {Punj, Deep and Mivelle, Mathieu and Moparthi, Satish Babu
		  and Zanten, Thomas S. van and Rigneault, Hervé and Hulst,
		  Niek F. van and García-Parajó, María F. and Wenger,
