@Article{	  weiss1999fluorescence,
  title		= {Fluorescence spectroscopy of single biomolecules.},
  author	= {Weiss, Shimon},
  journal	= {Science (New York, N.Y.)},
  year		= {1999},
  month		= {3},
  number	= {5408},
  pages		= {1676--83},
  volume	= {283},
  abstract	= {Recent advances in single-molecule detection and
		  single-molecule spectroscopy at room temperature by
		  laser-induced fluorescence offer new tools for the study of
		  individual macromolecules under physiological conditions.
		  These tools relay conformational states, conformational
		  dynamics, and activity of single biological molecules to
		  physical observables, unmasked by ensemble averaging.
		  Distributions and time trajectories of these observables
		  can therefore be measured during a reaction without the
		  impossible need to synchronize all the molecules in the
		  ensemble. The progress in applying these tools to
		  biological studies with the use of fluorophores that are
		  site-specifically attached to macromolecules is reviewed.},
  doi		= {10.1126/science.283.5408.1676},
  issn		= {0036-8075},
  owner		= {biswajit},
  timestamp	= {2017.09.05},
  url		= {http:https://doi.org/10.1126/science.283.5408.1676}
}

