@Article{	  hughes2014choose,
  title		= {Choose Your Label Wisely: Water-Soluble Fluorophores Often
		  Interact with Lipid Bilayers},
  author	= {Hughes, Laura D. and Rawle, Robert J. and Boxer, Steven
		  G.},
  journal	= {PLoS ONE},
  year		= {2014},
  number	= {2},
  volume	= {9},
  abstract	= {Water-soluble organic fluorophores are widely used as
		  labels in biological systems. However, in many cases these
		  fluorophores can interact strongly with lipid bilayers,
		  influencing the interaction of the target with the bilayer
		  and/or leading to misleading fluorescent signals. Here, we
