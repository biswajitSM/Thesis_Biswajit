  pmid		= {21969548},
  publisher	= {Proceedings of the National Academy of Sciences of the
		  United States of America},
  timestamp	= {2017.12.20},
  url		= {http://dx.doi.org/10.1073/pnas.1113572108}
}

@Article{	  goodman2016understanding,
  title		= {Understanding Resonant Light-Triggered DNA Release from
		  Plasmonic Nanoparticles},
  author	= {Goodman, Amanda M. and Hogan, Nathaniel J. and Gottheim,
		  Samuel and Li, Carrie and Clare, Susan E. and Halas, Naomi
		  J.},
  journal	= {ACS Nano},
  year		= {2016},
  number	= {1},
  pages		= {171--179},
  volume	= {11},
  abstract	= {Nanoparticle-based platforms for gene therapy and drug
		  delivery are gaining popularity for cancer treatment. To
		  improve therapeutic selectivity, one important strategy is
		  to remotely trigger the release of a therapeutic cargo from
		  a specially designed gene- or drug-laden near-infrared
		  (NIR) absorbing gold nanoparticle complex with NIR light.
		  While there have been multiple demonstrations of NIR
		  nanoparticle-based release platforms, our understanding of
		  how light-triggered release works in such complexes is
		  still limited. Here, we investigate the specific mechanisms
		  of DNA release from plasmonic nanoparticle complexes using
		  continuous wave (CW) and femtosecond pulsed lasers. We find
		  that the characteristics of nanoparticle-based DNA release
		  vary profoundly from the same nanoparticle complex,
		  depending on the type of laser excitation. CW laser
		  illumination drives the photothermal release of
		  dehybridized single-stranded DNA, while pulsed-laser
		  excitation results in double-stranded DNA release by
		  cleavage of the Au–S bond, with negligible local heating.
		  This dramatic difference in DNA release from the same
		  DNA–nanoparticle complex has very important implications
		  in the development of NIR-triggered gene or drug delivery
		  nanocomplexes.},
  doi		= {10.1021/acsnano.6b06510},
  issn		= {1936-0851},
  owner		= {biswajit},
  pmid		= {28114757},
  publisher	= {ACS Nano},
  timestamp	= {2017.12.08},
  url		= {http://dx.doi.org/10.1021/acsnano.6b06510}
}

